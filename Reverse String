class Main {
    public static void String reverse(String s){
        String reversedString="";
        int n=s.length();
        for(int i=0;i<n;i++)
        {
            reversedString=s.charAt(i) + reversedString;
            
        }
        return reversedString;
    }
    public static void main(String[] args) {
            String str="hello";
            System.out.println(reverse(str));    }
}
