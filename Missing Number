public class Main
{
    
	public static void main(String[] args) {
        int[] arr = new int[]{1, 2, 4, 5, 6, 7};  // Java infers the size from the number of elements
        int n=7;
        int sum=(n*(n+1))/2;
        int arrSum=0;
        for(int i=0;i<arr.length;i++){
            arrSum+=arr[i];
        }
        int missingNumber=sum-arrSum;
        System.out.println(missingNumber);
        
        
    }
}
